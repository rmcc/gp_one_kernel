--- drivers/mmc/core/core.c
+++ drivers/mmc/core/core.c
@@ -1306,14 +1306,20 @@
 		cancel_delayed_work(&host->disable);
 
 	mmc_bus_get(host);
+#ifndef CONFIG_MACH_ADQ
 	if (host->bus_ops && !host->bus_dead) {
 		if (host->bus_ops->suspend)
 			err = host->bus_ops->suspend(host);
 	}
+#endif
 	mmc_bus_put(host);
 
+#ifndef CONFIG_MACH_ADQ
 	if (!err)
 		mmc_power_off(host);
+#else
+    err = 0;
+#endif
 
 	return err;
 }
